<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Students</title>
  </head>
  <body>
    <table border="1">
      <tr>

        <th>Full Name</th>
        <th>Email</th>
      </tr>
      <%dataStudents.forEach((dataparams)=>{%>
        <tr>
          <td><%=dataparams.getFullname()%></td>
          <td><%=dataparams.email%></td>
          <td><a href="/students/edit/<%=dataparams.id%>"> Edit </td>
          <td><a href="/students/delete/<%dataparams.id%>"> Delete </td>
        </tr>
      <%})%>
    </table>
    <a href="/teachers/add">Add Students</a>
  </body>
</html>
